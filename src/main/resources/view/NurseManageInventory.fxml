         <font>
            <Font name="Cambria Math" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="136.0" fitWidth="138.0" layoutX="15.0" layoutY="9.0" pickOnBounds="true">
         <image>
            <Image url="@../images/pharmacy.png" />
         </image>
      </ImageView>
      <VBox layoutX="40.0" layoutY="156.0" prefHeight="122.0" prefWidth="120.0" spacing="5.0">
         <children>
            <Label prefHeight="25.0" prefWidth="215.0" text="INVENTORY ID" textFill="WHITE">
               <font>
                  <Font name="Cambria Math" size="14.0" />
               </font>
            </Label>
            <Label layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="215.0" text="QUANTITY" textFill="WHITE">
               <font>
                  <Font name="Cambria Math" size="14.0" />
