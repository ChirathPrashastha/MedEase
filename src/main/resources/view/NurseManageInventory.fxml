            </Label>
            <Label layoutX="10.0" layoutY="40.0" prefHeight="25.0" prefWidth="215.0" text="SUPPLIER ID" textFill="WHITE">
               <font>
                  <Font name="Cambria Math" size="14.0" />
               </font>
            </Label>
            <Label layoutX="10.0" layoutY="70.0" prefHeight="25.0" prefWidth="215.0" text="SECTION" textFill="WHITE">
               <font>
                  <Font name="Cambria Math" size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="172.0" layoutY="154.0" prefHeight="129.0" prefWidth="229.0" spacing="4.0">
         <children>
            <TextField fx:id="txtInventoryId" onAction="#getMedicineDetailsOnAction" prefHeight="25.0" styleClass="input-fields" stylesheets="@../css/NurseDashboardStyle.css" />
            <TextField fx:id="txtQty" layoutX="10.0" layoutY="10.0" prefHeight="25.0" styleClass="input-fields" stylesheets="@../css/NurseDashboardStyle.css" />
            <TextField fx:id="txtSupplierId" layoutX="10.0" layoutY="35.0" prefHeight="25.0" styleClass="input-fields" stylesheets="@../css/NurseDashboardStyle.css" />
            <TextField fx:id="txtSection" layoutX="10.0" layoutY="60.0" prefHeight="25.0" styleClass="input-fields" stylesheets="@../css/NurseDashboardStyle.css" />
         </children>
      </VBox>
      <TableView fx:id="tblInventory" layoutX="36.0" layoutY="349.0" prefHeight="233.0" prefWidth="696.0">
        <columns>
          <TableColumn fx:id="colInventoryId" prefWidth="75.0" text="Invetory Id" />
            <TableColumn fx:id="colQuantity" prefWidth="75.0" text="Quantity" />
            <TableColumn fx:id="colSupplierId" prefWidth="75.0" text="Supplier Id" />
            <TableColumn fx:id="colSection" prefWidth="75.0" text="Section" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER_LEFT" layoutX="172.0" layoutY="289.0" prefHeight="45.0" prefWidth="288.0" spacing="20.0">
