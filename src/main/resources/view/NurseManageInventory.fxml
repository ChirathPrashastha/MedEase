                  <Font name="Cambria Bold" size="11.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="116.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefHeight="25.0" prefWidth="71.0" styleClass="delete-btn" stylesheets="@../css/NurseDashboardStyle.css" text="DELETE">
