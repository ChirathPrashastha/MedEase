            <Button mnemonicParsing="false" onAction="#btnAddOnAction" prefHeight="25.0" prefWidth="71.0" styleClass="add-btn" stylesheets="@../css/NurseDashboardStyle.css" text="ADD">
               <font>
                  <Font name="Cambria Bold" size="11.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#btnUpdateOnAction" prefHeight="25.0" prefWidth="71.0" styleClass="update-btn" stylesheets="@../css/NurseDashboardStyle.css" text="UPDATE">
