            <Label layoutX="21.0" layoutY="56.0" prefHeight="25.0" prefWidth="97.0" text="MEDICINE ID" textFill="WHITE">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Label>
            <Label layoutX="21.0" layoutY="88.0" prefHeight="25.0" prefWidth="90.0" text="NAME" textFill="WHITE">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Label>
            <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="19.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/light-bulb.png" />
               </image>
            </ImageView>
            <Label fx:id="lblMedName" layoutX="129.0" layoutY="88.0" prefHeight="25.0" prefWidth="97.0" textFill="WHITE">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblMID" layoutX="132.0" layoutY="57.0" prefHeight="25.0" prefWidth="68.0" textFill="WHITE">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="504.0" layoutY="293.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/reorder.png" />
         </image>
      </ImageView>
      <Label layoutX="540.0" layoutY="296.0" prefHeight="25.0" prefWidth="127.0" text="RESTOCK REQUEST" textFill="WHITE">
         <font>
            <Font name="Cambria Math" size="13.0" />
         </font>
      </Label>
      <Button layoutX="494.0" layoutY="290.0" mnemonicParsing="false" onAction="#btnRequestRestockOnAction" prefHeight="39.0" prefWidth="178.0" style="-fx-background-color: transparent;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
